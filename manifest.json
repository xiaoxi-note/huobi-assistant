{
  "name": "huobi-assistant",
  "manifest_version": 2,
  "version": "1.2",
  "description": "同步数据",
  "browser_action": {
    "default_icon": "favicon.ico"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "pako.js",
        "script.js"
      ],
      "css": [
        "base.css"
      ]
    }
  ],
  "permissions": [
    "http://*/",
    "storage"
  ]
}